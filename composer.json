{
    "name": "kornrunner/ethereum-offline-tx",
    "description": "Pure PHP Ethereum Offline Transaction Signer",
    "keywords": ["ethereum", "offline", "transaction"],
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Momcilovic",
            "homepage": "https://github.com/kornrunner/php-ethereum-offline-tx"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "kornrunner/keccak": "^1.0",
        "web3p/rlp": "^0.2.1",
        "web3p/secp256k1": "^0.2.0",
        "simplito/elliptic-php": "^1.0",
        "ext-gmp": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "kornrunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kornrunner\\": "test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
